{
  "name": "@digifarm/core",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript library providing core functionality for the Digifarm API services, including AWS service integrations, data access patterns, and utility functions.",
  "homepage": "https://github.com/digifarmio/digifarm-core#readme",
  "bugs": {
    "url": "https://github.com/digifarmio/digifarm-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digifarmio/digifarm-core.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=10.0.0",
    "yarn": "please-use-npm"
  },
  "license": "MIT",
  "author": "Digifarm",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@slack/web-api": "^6.9.0",
    "aws-sdk": "^2.1692.0",
    "bluebird": "^3.7.2",
    "geojson": "^0.5.0",
    "lambda-log": "^3.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@types/aws-lambda": "^8.10.150",
    "@types/bluebird": "^3.5.42",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/lambda-log": "^3.0.3",
    "@types/lodash": "^4.17.20",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
