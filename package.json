{
  "name": "@digifarmio/core",
  "version": "0.1.3",
  "description": "A comprehensive TypeScript library providing core functionality for the Digifarm API services, including AWS service integrations, data access patterns, and utility functions.",
  "homepage": "https://github.com/digifarmio/digifarm-core#readme",
  "bugs": {
    "url": "https://github.com/digifarmio/digifarm-core/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digifarmio/digifarm-core.git"
  },
  "engines": {
    "node": ">=18.20.8",
    "npm": ">=10.8.2",
    "yarn": "please-use-npm"
  },
  "license": "MIT",
  "author": "Digifarm",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src/**/*",
    "lint:fix": "eslint src/**/* --fix",
    "version:patch": "ts-node scripts/bump-version.js patch",
    "version:minor": "ts-node scripts/bump-version.js minor",
    "version:major": "ts-node scripts/bump-version.js major"
  },
  "dependencies": {
    "@slack/web-api": "^6.9.0",
    "aws-sdk": "^2.1692.0",
    "bluebird": "^3.7.2",
    "geojson": "^0.5.0",
    "lambda-log": "^3.1.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@jest/globals": "^30.0.4",
    "@types/aws-lambda": "^8.10.150",
    "@types/bluebird": "^3.5.42",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/lambda-log": "^3.0.3",
    "@types/lodash": "^4.17.20",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "globals": "^16.3.0",
    "jest": "^30.0.4",
    "prettier": "3.6.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
